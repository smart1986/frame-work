<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" monitorInterval="1"
	shutdownHook="disable" name="RocketmqClient">
	<Properties>
		<Property name="LOG_HOME">log/game</Property>
	</Properties>
	<Appenders>
		<Console name="stdout" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %l - %msg%n" />
		</Console>

	</Appenders>


	<Loggers>
		<logger name="org.springframework" level="error">
		</logger>

		<Root level="debug">
			<AppenderRef ref="stdout" />
		</Root>
	</Loggers>
</Configuration>
